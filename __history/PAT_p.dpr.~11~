program PAT_p;







uses
  Vcl.Forms,
  logicEditor in 'logicEditor.pas' {frmGame},
  mainScreen_u in 'mainScreen_u.pas' {frmStart},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Charcoal Dark Slate');
  Application.Title := 'Learn To Logic';
  Application.CreateForm(TfrmStart, frmStart);
  Application.CreateForm(TfrmGame, frmGame);
  Application.Run;
end.
